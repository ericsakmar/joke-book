datasource DS {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Joke {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  setUp     String
  punchLine String
  name      String
  upVotes   Int
  downVotes Int
}
